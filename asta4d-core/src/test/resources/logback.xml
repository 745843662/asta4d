<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<!-- Appenders -->
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36}[%line] - %msg%n</pattern>
        </encoder>
    </appender>

	<!-- Root Logger -->
	<root level="debug">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
